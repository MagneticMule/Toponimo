<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:bottom="1dip">
        <shape>
            <solid android:color="#969696" >
                <corners android:radius="2dip" />
            </solid>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#88888888" />

            <corners android:radius="2dp" />
        </shape>
    </item>
    <item
        android:left="1dip"
        android:top="1dip">
        <shape android:dither="true" >
            <gradient
                android:angle="270"
                android:centerColor="#fafafa"
                android:endColor="#fafafa"
                android:startColor="#f1f1f1" />

            <stroke
                android:width="1dp"
                android:color="#88888888" />

            <corners android:radius="2dp" />
        </shape>
    </item>

</layer-list>